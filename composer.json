{
    "name": "czim/laravel-cms-theme",
    "description": "Laravel CMS: Theme component",
    "keywords": [ "cms", "laravel", "theme", "view", "assets" ],
    "homepage": "https://github.com/czim",
    "require": {
        "php": ">=5.5.9"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "mockery/mockery": "0.9.*",
        "scrutinizer/ocular": "~1.1",
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Czim\\CmsTheme\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Czim\\CmsTheme\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
